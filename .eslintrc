{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "react/prop-types": 0,
        "react/display-name": 0,
        "react/no-unescaped-entities": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_$" }]
    }
}
